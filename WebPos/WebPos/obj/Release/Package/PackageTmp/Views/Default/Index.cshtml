@model WebPos.Models.DefaultModel
@using Newtonsoft.Json
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <link href="~/Styles/Style.css" rel="stylesheet" />
    <script src="~/scripts/JavaScript.js"></script>
    <title></title>
    <meta charset="utf-8" />
    <script type="text/javascript">

        var data = @Html.Raw(JsonConvert.SerializeObject(this.Model));

    </script>
</head>
<body>
    @using (Html.BeginForm(null, null, FormMethod.Post, new { ID = "MainForm", onsubmit = "validateForm()" }))
    {
        @Html.AntiForgeryToken()
        <div id="header">
            <img src="data:image/png;base64," />
            <div>ePokladna</div>
            <p>Číslo dokladu: 123654789</p>
        </div>

        <div class="main_block">

            <div id="shop_div">
                <header>Prodejna</header>
                <div>
                    @Html.DropDownListFor(model => model.Store.Name, new SelectList(Model.shopList, "Number", "Text"), "", new {required="required", @onchange = "shop_onChange(this.value, data)" })
                    @Html.TextBoxFor(model => model.Store.Street, new { @disabled = "disabled" })
                    @Html.TextBoxFor(model => model.Store.City, new { @disabled = "disabled" })
                    @Html.TextBoxFor(model => model.Store.ZIP, new { @disabled = "disabled" })
                    <table>
                        <tr>
                            <td class="col1">IČ:</td>
                            <td>@Html.TextBoxFor(model => model.Store.IC, new { @disabled = "disabled" })</td>
                        </tr>
                        <tr>
                            <td class="col1">DIČ:</td>
                            <td>@Html.TextBoxFor(model => model.Store.DIC, new { @disabled = "disabled" })</td>
                        </tr>
                    </table>
                </div>
            </div>

            <div id="customer_div">
                <header>Zákazník</header>
                <div>
                    <table>
                        <tr>
                            <td>Jméno:</td>
                            <td>@Html.TextBoxFor(model => model.Customer.FirstName, new { onfocus = "input_onFocus(this)", @maxlength = "20" })</td>
                            <td>Přijímení:</td>
                            <td>@Html.TextBoxFor(model => model.Customer.LastName, new { onfocus = "input_onFocus(this)", @maxlength = "20" })</td>
                        </tr>
                        <tr>
                            <td>Firma:</td>
                            <td>@Html.TextBoxFor(model => model.Customer.Company, new { onfocus = "input_onFocus(this)", @maxlength = "20" })</td>
                        </tr>
                        <tr>
                            <td>Ulice:</td>
                            <td>@Html.TextBoxFor(model => model.Customer.Street, new { onfocus = "input_onFocus(this)", @maxlength = "20" })</td>
                            <td>č.p.:</td>
                            <td>@Html.TextBoxFor(model => model.Customer.Number, new { onfocus = "input_onFocus(this)", @maxlength = "20" })</td>
                        </tr>
                        <tr>
                            <td>Město:</td>
                            <td>@Html.TextBoxFor(model => model.Customer.City, new { onfocus = "input_onFocus(this)", @maxlength = "20" })</td>
                            <td>PSČ:</td>
                            <td>@Html.TextBoxFor(model => model.Customer.ZIP, new { onfocus = "input_onFocus(this)", @maxlength = "6", @pattern = "\\d{3} ?\\d{2}" })</td>
                        </tr>
                        <tr>
                            <td>IČ:</td>
                            <td>@Html.TextBoxFor(model => model.Customer.IC, new { onfocus = "input_onFocus(this)", onchange = "validation_IC()", @maxlength = "8", @pattern = "[0-9]{8}" })</td>
                            <td>DIČ:</td>
                            <td>@Html.TextBoxFor(model => model.Customer.DIC, new { onfocus = "input_onFocus(this)", @maxlength = "20" })</td>
                        </tr>
                        <tr>
                            <td>Telefon:</td>
                            <td>@Html.TextBoxFor(model => model.Customer.PhoneNumber, new { onfocus = "input_onFocus(this)", @maxlength = "20", @pattern = "^(\\+420)? ?[1-9][0-9]{2} ?[0-9]{3} ?[0-9]{3}$", @type = "tel", @placeholder = "+420 800 770 077" })</td>
                            <td>Email:</td>
                            <td>@Html.TextBoxFor(model => model.Customer.Email, new { onfocus = "input_onFocus(this)", @maxlength = "20", @type = "email", @placeholder = "john.doe@example.com" })</td>
                        </tr>
                    </table>
                </div>

            </div>

        </div>

        <table id="articles">
            <thead>
                <tr>
                    <td>Číslo zboží</td>
                    <td>Popis</td>
                    <td>Množství</td>
                    <td>Cena</td>
                    <td>DPH</td>
                    <td>Celkem</td>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td class="col1">@Html.DropDownListFor(model => model.Item1.GUID, new SelectList(Model.articleList, "UID", "VART"), "", new { @onchange = "article_onChange(this.value, 1, data)" })</td>
                    <td class="col1">@Html.TextBoxFor(model => model.Item1.Description, new { disabled = "disabled", onfocus = "input_onFocus(this)", @maxlength = "220" })</td>
                    <td>@Html.TextBoxFor(model => model.Item1.Quantity, new { @class= "number_input", disabled = "disabled", oninput = "number_input(this)", onchange = "number_onchange(this)", onfocus = "input_onFocus(this)" })</td>
                    <td>@Html.TextBoxFor(model => model.Item1.Price, new { @class = "number_input", disabled = "disabled", oninput = "number_input(this)", onchange = "number_onchange(this)", onfocus = "input_onFocus(this)" })</td>
                    <td>@Html.TextBoxFor(model => model.Item1.VAT, new { @class = "number_input", disabled = "disabled" })</td>
                    <td class="col2">@Html.TextBoxFor(model => model.Item1.Total, new { @class = "number_input", disabled = "disabled" })</td>
                </tr>
                <tr>
                    <td class="col1">@Html.DropDownListFor(model => model.Item2.GUID, new SelectList(Model.articleList, "UID", "VART"), "", new { @onchange = "article_onChange(this.value, 2, data)" })</td>
                    <td class="col1">@Html.TextBoxFor(model => model.Item2.Description, new { disabled = "disabled", onfocus = "input_onFocus(this)", @maxlength = "220" })</td>
                    <td>@Html.TextBoxFor(model => model.Item2.Quantity, new { @class = "number_input", disabled = "disabled", oninput = "number_input(this)", onchange = "number_onchange(this)", onfocus = "input_onFocus(this)" })</td>
                    <td>@Html.TextBoxFor(model => model.Item2.Price, new { @class = "number_input", disabled = "disabled", oninput = "number_input(this)", onchange = "number_onchange(this)", onfocus = "input_onFocus(this)" })</td>
                    <td>@Html.TextBoxFor(model => model.Item2.VAT, new { @class = "number_input", disabled = "disabled" })</td>
                    <td class="col2">@Html.TextBoxFor(model => model.Item2.Total, new { @class = "number_input", disabled = "disabled" })</td>
                </tr>
                <tr>
                    <td class="col1">@Html.DropDownListFor(model => model.Item3.GUID, new SelectList(Model.articleList, "UID", "VART"), "", new { @onchange = "article_onChange(this.value, 3, data)" })</td>
                    <td class="col1">@Html.TextBoxFor(model => model.Item3.Description, new { disabled = "disabled", onfocus = "input_onFocus(this)", @maxlength = "220" })</td>
                    <td>@Html.TextBoxFor(model => model.Item3.Quantity, new { @class = "number_input", disabled = "disabled", oninput = "number_input(this)", onchange = "number_onchange(this)", onfocus = "input_onFocus(this)" })</td>
                    <td>@Html.TextBoxFor(model => model.Item3.Price, new { @class = "number_input", disabled = "disabled", oninput = "number_input(this)", onchange = "number_onchange(this)", onfocus = "input_onFocus(this)" })</td>
                    <td>@Html.TextBoxFor(model => model.Item3.VAT, new { @class = "number_input", disabled = "disabled" })</td>
                    <td class="col2">@Html.TextBoxFor(model => model.Item3.Total, new { @class = "number_input", disabled = "disabled" })</td>
                </tr>
                <tr>
                    <td class="col1">@Html.DropDownListFor(model => model.Item4.GUID, new SelectList(Model.articleList, "UID", "VART"), "", new { @onchange = "article_onChange(this.value, 4, data)" })</td>
                    <td class="col1">@Html.TextBoxFor(model => model.Item4.Description, new { disabled = "disabled", onfocus = "input_onFocus(this)", @maxlength = "220" })</td>
                    <td>@Html.TextBoxFor(model => model.Item4.Quantity, new { @class = "number_input", disabled = "disabled", oninput = "number_input(this)", onchange = "number_onchange(this)", onfocus = "input_onFocus(this)" })</td>
                    <td>@Html.TextBoxFor(model => model.Item4.Price, new { @class = "number_input", disabled = "disabled", oninput = "number_input(this)", onchange = "number_onchange(this)", onfocus = "input_onFocus(this)" })</td>
                    <td>@Html.TextBoxFor(model => model.Item4.VAT, new { @class = "number_input", disabled = "disabled" })</td>
                    <td class="col2">@Html.TextBoxFor(model => model.Item4.Total, new { @class = "number_input", disabled = "disabled" })</td>
                </tr>
            </tbody>
        </table>

        <div id="totals_div">
            <header>
                <table>
                    <tr>
                        <td>Celkem:</td>
                        <td><p id="total">0.00</p></td>
                        <td> Kč</td>
                    </tr>
                    <tr>
                        <td>Celkem k úhradě:</td>
                        <td><p id="total_for_payment">0.00</p></td>
                        <td> Kč</td>
                    </tr>
                </table>
            </header>
            <div>
                <table id="payments_table">
                    <tbody>
                        <tr>
                            <td> @Html.DropDownListFor(model => model.Payments[0].Type, new SelectList(Model.paymentTypeList, "Type", "Description"), "", new { @onchange = "paymentType_onChange(this, data)" })</td>
                            <td> @Html.TextBoxFor(model => model.Payments[0].Value, new { disabled = "disabled", onchange = "recalculateTotals()", onfocus = "input_onFocus(this)" })</td>
                            <td> <button onclick="deletePayment_onClick(this); return false;"></button></td>
                        </tr>
                    </tbody>
                    <tfoot>
                        <tr><td><button onclick="addPayment_onClick(data); return false">Přidat platbu</button></td></tr>
                    </tfoot>
                </table>
                <table>
                    <tr>
                        <td><p>Zaplaceno:</p></td>
                        <td><input id="total_paid" disabled readonly /></td>
                    </tr>
                    <tr>
                        <td><p>Zbývá zaplatit:</p></td>
                        <td><input id="total_to_paid" disabled readonly /></td>
                    </tr>
                    <tr>
                        <td><p>Vrátit:</p></td>
                        <td><input id="total_to_return" disabled readonly /></td>
                    </tr>
                </table>
            </div>
        </div>

        <button type="submit">Potvrdit</button>
    }
</body>
</html>
